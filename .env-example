# Tebra Debug Dashboard Environment Variables
# Copy this file to .env and fill in your values

# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-auth0-domain.us.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_REDIRECT_URI=http://localhost:5173
VITE_AUTH0_AUDIENCE=https://api.patientflow.com
VITE_AUTH0_SCOPE="openid profile email"

# Tebra Integration
VITE_TEBRA_USERNAME=your-tebra-username
VITE_TEBRA_PASSWORD=your-tebra-password
VITE_TEBRA_CUSTOMER_KEY=your-customer-key
VITE_TEBRA_WSDL_URL=https://webservice.kareo.com/services/soap/2.1/KareoServices.svc?wsdl&customerkey=your-key
VITE_TEBRA_PROXY_API_KEY=your-proxy-api-key
VITE_TEBRA_CLOUD_RUN_URL=https://your-tebra-php-api.a.run.app

# Firebase Configuration
VITE_FIREBASE_CONFIG={"projectId":"your-project","apiKey":"your-key"}
GOOGLE_CLOUD_PROJECT=your-firebase-project
FIREBASE_PROJECT_ID=your-firebase-project

# Patient Encryption
VITE_PATIENT_ENCRYPTION_KEY=your-base64-encryption-key

# Debug Dashboard Configuration (optional - uses defaults if not set)
VITE_TEBRA_DEBUG_INTERVAL=10000
VITE_TEBRA_DEBUG_TIMEOUT=10000

# Application settings
VITE_APP_NAME=Patient Flow Management 