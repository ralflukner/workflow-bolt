{
    "name": "luknerlumina/tebra-php-api",
    "description": "PHP Cloud Run service for Tebra API integration",
    "type": "project",
    "version": "1.0.1",
    "require": {
        "php": "^8.2",
        "ext-soap": "*",
        "ext-json": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^3.0",
        "open-telemetry/opentelemetry": "^1.0",
        "google/cloud-secret-manager": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.13.1"
    },
    "autoload": {
        "psr-4": {
            "LuknerLumina\\TebraApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LuknerLumina\\TebraApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
} 